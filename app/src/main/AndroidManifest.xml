<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.getkracking">
    <uses-permission
        android:name="android.permission.INTERNET" />
    <application
        android:name=".app.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_pulpetochonk"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_pulpetochonk"
        android:supportsRtl="true"
        android:theme="@style/Theme.Getkracking"
        android:networkSecurityConfig="@xml/network_security_config">
        <activity android:name=".HomeActivity" android:windowSoftInputMode="adjustPan"/>
        <activity android:name=".WelcomeActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>